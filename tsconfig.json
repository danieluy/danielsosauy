{
  "compilerOptions": {
    "outDir": "public",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ],
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}